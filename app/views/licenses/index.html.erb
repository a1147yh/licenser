<h1>Listing licenses</h1>

<table>
  <tr>
    <th>Product</th>
    <th>User Id</th>
    <th>User Name</th>
    <th>Begining date</th>
    <th>Ending date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @licenses.each do |license| %>
  <tr>
    <td><%= license.product.name %></td>
    <td><%= license.user_id  %></td>
    <td><%= license.user.user_name  %></td>
    <td><%= license.begining_date %></td>
    <td><%= license.ending_date %></td>
    <td><%= link_to 'Show', license %></td>
    <td><%= link_to 'Edit', edit_license_path(license) %></td>
    <td><%= link_to 'Destroy', license, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New License', new_license_path %>
